<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icons\bar_chart.png" />
    <None Remove="icons\egg.jpg" />
    <None Remove="icons\line_chart.png" />
    <None Remove="icons\line_chart.svg" />
    <None Remove="icons\logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons\logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons\bar_chart.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons\line_chart.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="LiveCharts.Wpf" Version="0.9.7" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icons\egg.jpg" />
    <Resource Include="icons\line_chart.svg" />
  </ItemGroup>

</Project>
